{
 "name": "volumer",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Volume control for Chrome",
  "action": {
    "default_icon": "./assets/icon.png",
    "default_popup": "./src/popup.html"
  },
  "permissions": [
    "tabs",
    "scripting",
    "tabCapture"
  ],
  "background": {
    "service_worker": "./src/service_worker.js"
  }
}